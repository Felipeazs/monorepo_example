{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "incremental": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "jsx": "react-jsx",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "@/client/*": ["./src/*"]
        }
    },
    "include": ["src"]
}
